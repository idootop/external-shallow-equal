{
  "name": "react-shallowequal",
  "version": "1.0.0",
  "description": "Equivalent to React's internal shallowEqual, allowing external access to React's shallowEqual function.",
  "homepage": "https://github.com/idootop/react-shallowequal",
  "bugs": {
    "url": "https://github.com/idootop/react-shallowequal/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/idootop/react-shallowequal"
  },
  "author": {
    "name": "Del Wang",
    "email": "hello@xbox.work",
    "url": "https://github.com/idootop"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "shallowEqual",
    "react-addons-shallow-compare"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc --module CommonJS && cd dist && mv \"index.js\" \"index.cjs\" && cd ../ && tsc --module esnext"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs",
    "default": "./dist/index.js"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typescript": "^5.2.2"
  }
}
